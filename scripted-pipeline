                sh 'ls -l build.xml'
                sh 'cat build.xml'
          }
	 	 //build job: 'document-roshambo', parameters: [booleanParam(name: 'generate_javadoc', value: false), stringParam(name: 'javadoc_location', value: 'C:\\_javadoc00')]

   	}

